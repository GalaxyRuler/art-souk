{
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "globals": {
    "NodeJS": "readonly",
    "globalThis": "readonly"
  },
  "rules": {
    "no-undef": "off",
    "@typescript-eslint/no-explicit-any": "warn",
    "@typescript-eslint/no-empty-object-type": "off"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-undef": "off"
      }
    }
  ]
}